<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Set up AutoWiring -->
	<bean id="house" class="com.revature.beans.House" scope="prototype" autowire="constructor">
	</bean>

	<bean id="brokenWindow" class="com.revature.beans.BrokenWindow" scope="singleton"/>
	<bean id="window" class="com.revature.beans.StandardWindow" scope="prototype"/>

</beans>
